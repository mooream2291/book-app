<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>newBookArr</title>
    <link rel="stylesheet" href="/styles/layout.css">
</head>
<body>
    <!-- shows results from homepagesearch -->
    <!-- select.png wireframe -->
    <main>
    <h1>Best Books!</h1>
    <h2>Search Results</h2>
    <ul>
        <% results.forEach(book => { %>
        <li>
